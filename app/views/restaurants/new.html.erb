<div class='container'>
  <h1>New Restaurant</h1>
  <%= simple_form_for @restaurant do |f| %>
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :phone_number %>
    <%= f.input :category %>
    <%= f.submit "Submit review", class: "btn btn-primary" %>
  <% end %>
  <p><%= link_to 'Home Page!', restaurants_path %></p>
</div>
